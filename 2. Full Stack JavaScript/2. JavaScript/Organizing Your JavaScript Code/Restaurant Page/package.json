{
  "name": "odin-restaurant_page",
  "version": "1.0.0",
  "description": "Exercise for the Odin Project - Full-stack JavaScript on ES6 Modules and Webpack",
  "private": "true",
  "scripts": {
    "dev": "webpack serve --open-app-name 'Firefox Developer Edition' --config webpack.dev.js",
    "lint": "eslint src/",
    "lint:fix": "eslint --fix src/",
    "pretty": "prettier --check src/",
    "pretty:fix": "prettier --write src/",
    "check": "npm run lint & npm run pretty",
    "fix": "npm run lint:fix & npm run pretty:fix",
    "build": "webpack --config webpack.prod.js",
    "deploy": "git subtree push --prefix dist origin gh-pages"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/auliviet/odin-restaurant_page.git"
  },
  "author": "Auliviet",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/auliviet/odin-restaurant_page/issues"
  },
  "homepage": "https://github.com/auliviet/odin-restaurant_page#readme",
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^7.1.2",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "html-loader": "^5.0.0",
    "html-webpack-plugin": "^5.6.0",
    "prettier": "3.3.3",
    "style-loader": "^4.0.0",
    "webpack": "^5.92.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4",
    "webpack-merge": "^6.0.1"
  }
}
